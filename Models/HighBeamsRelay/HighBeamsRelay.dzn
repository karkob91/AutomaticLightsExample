interface IHighBeamsRelay {
    in void TurnOn();
    in void TurnOff();
    

    behavior {
        bool isTurnedOn = false;

        [isTurnedOn] {
            
            on TurnOff: {
                isTurnedOn = false;
            }
        }
        [!isTurnedOn] {
            on TurnOn: {
                isTurnedOn = true;
            }
            
        }
    }

}